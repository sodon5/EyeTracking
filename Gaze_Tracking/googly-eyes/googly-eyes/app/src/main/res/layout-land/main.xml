<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/topLayout"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:keepScreenOn="true">

    <com.google.android.gms.samples.vision.face.googlyeyes.ui.camera.CameraSourcePreview
      android:id="@+id/preview"
      android:layout_width="fill_parent"
      android:layout_height="fill_parent">

    <com.google.android.gms.samples.vision.face.googlyeyes.ui.camera.GraphicOverlay
        android:id="@+id/faceOverlay"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"

        />

  </com.google.android.gms.samples.vision.face.googlyeyes.ui.camera.CameraSourcePreview>

  <Button
      android:id="@+id/button2"
      android:layout_width="50dp"
      android:layout_height="50dp"
      android:layout_alignParentLeft="true"
      android:layout_alignParentStart="true"
      android:layout_alignParentTop="true"
      android:background="@drawable/button_click" />

  <Button
      android:id="@+id/button3"
      android:layout_width="50dp"
      android:layout_height="50dp"
      android:layout_alignParentBottom="true"
      android:layout_alignParentLeft="true"
      android:layout_alignParentStart="true"
      android:background="@drawable/button_click" />

  <Button
      android:id="@+id/button4"
      android:layout_width="50dp"
      android:layout_height="50dp"
      android:background="@drawable/button_click"
      android:layout_alignParentTop="true"
      android:layout_alignParentRight="true"
      android:layout_alignParentEnd="true" />

  <Button
      android:id="@+id/button5"
      android:layout_width="50dp"
      android:layout_height="50dp"
      android:background="@drawable/button_click"
      android:layout_alignParentBottom="true"
      android:layout_alignParentRight="true"
      android:layout_alignParentEnd="true" />



    <TableLayout
        android:id="@+id/table"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:stretchColumns="3"
        android:visibility="visible">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <View
                    android:id="@+id/one_one"
                    android:layout_width="800px"
                    android:layout_height="400px"
                    android:background="#EE8484"
                    android:visibility="invisible" />
            </FrameLayout>


            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <View
                    android:id="@+id/one_two"
                    android:layout_width="800px"
                    android:layout_height="400px"
                    android:background="#EB5F5F"
                    android:visibility="invisible" />

            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <View
                    android:id="@+id/one_three"
                    android:layout_width="800px"
                    android:layout_height="400px"
                    android:background="#E90505"
                    android:visibility="invisible" />

            </FrameLayout>
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <View
                    android:id="@+id/two_one"
                    android:layout_width="800px"
                    android:layout_height="400px"
                    android:background="#DEE97A"
                    android:visibility="invisible"
                    />

            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <View
                    android:id="@+id/two_two"
                    android:layout_width="800px"
                    android:layout_height="400px"
                    android:background="#DAEB48"
                    android:visibility="invisible"
                    />

            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <View
                    android:id="@+id/two_three"
                    android:layout_width="800px"
                    android:layout_height="400px"
                    android:background="#258D17"
                    android:visibility="invisible"
                    />

            </FrameLayout>

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">


                <View
                    android:id="@+id/three_one"
                    android:layout_width="800px"
                    android:layout_height="400px"
                    android:background="#ABBDE8"
                    android:visibility="invisible"
                    />
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <View
                    android:id="@+id/three_two"
                    android:layout_width="800px"
                    android:layout_height="400px"
                    android:background="#577FDD"
                    android:visibility="invisible"
                    />
            </FrameLayout>

            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <View
                    android:id="@+id/three_three"
                    android:layout_width="800px"
                    android:layout_height="400px"
                    android:background="#255CDC"
                    android:visibility="invisible"
                    />
            </FrameLayout>

        </TableRow>

    </TableLayout>

</RelativeLayout>
